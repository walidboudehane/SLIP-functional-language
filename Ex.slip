2                                       ; ↝ 2
+                                       ; ↝ <function>
(+ 2 4)                                 ; ↝ 6

(add 1 (add 2 (add 3 (add 4))))         ; ↝ [1 2 3 . 4]

(let ((x 2)) (* x x))                   ; ↝ 4 
